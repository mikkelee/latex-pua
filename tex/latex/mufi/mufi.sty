\ProvidesPackage{mufi}[2019/04/25 MUFI Character Macros]

\RequirePackage{kvoptions}

\makeatletter

\def\mufi@nofont{}

\DeclareStringOption{font}{\mufi@nofont}
\DeclareBoolOption{mufifour}

\ProcessKeyvalOptions*

\ifx\mufi@font\mufi@nofont{%
	\PackageWarning{mufi}{Make sure your main font supports the MUFI characters; otherwise supply one that does with the font option.}
	\newcommand\mufi@changefont[1]{#1}%
\else%
	\RequirePackage{fontspec}
	\newfontface\mufi@changefont{\mufi@font}%
\fi%

\newcommand\mufi[1]{%
	{%
		\mufi@changefont{%
			\symbol{"#1}%
		}%
	}%
}

